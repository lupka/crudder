{
    "name": "lupka/crudder",
    "description": "Dead simple admin panels for Laravel.",
    "license": "MIT",
    "type": "package",
    "keywords": ["crudder", "crud", "laravel", "models"],
    "authors": [
        {
            "name": "Alex Chalupka",
            "email": "alex@alexchalupka.com"
        }
    ],
    "require": {
        "illuminate/support": "5.3.*",
        "laravel/framework": "5.3.*",
        "doctrine/dbal": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Lupka\\Crudder\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/CrudderTestCase.php"
        ],
        "psr-4": {
            "": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "mockery/mockery": "^0.9.5"
    }
}
